                <xs:alternative test="@role = 'andere zuständige Behörde'"
                    type="wgm.zustaendige_behoerde"/>
                <xs:alternative test="@role = 'andere Person oder Institution'"
                    type="wgm.person_institution"/>
            </xs:element>
            <xs:element name="name">
                <xs:alternative test="not(@role = 'Schadensarten' or @role = 'Provenienzforschung')"
