                <xs:alternative test="not(@role = 'andere Person oder Institution')" type="persname"/>
                <xs:alternative test="@role = 'andere Person oder Institution'"
                    type="wgm.person_institution"/>
            </xs:element>
            <xs:element name="subject" type="subject"/>
            <xs:element name="corpname">
                <xs:alternative
                    test="not(@role = 'andere zuständige Behörde' or @role = 'andere Person oder Institution')"
