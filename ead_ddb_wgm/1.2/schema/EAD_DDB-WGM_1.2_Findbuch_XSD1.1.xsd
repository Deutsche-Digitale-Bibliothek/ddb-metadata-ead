    <xs:complexType name="wgm.item">
        <xs:sequence>
            <xs:choice minOccurs="1" maxOccurs="unbounded">
                <xs:element name="name">
                    <xs:alternative test="@role = 'WGM-ID'" type="wgm.wgm_id"/>
                    <xs:alternative test="@role = 'Rolle'" type="wgm.rolle"/>
                    <xs:alternative test="@role = 'akademischer Titel'"
                        type="wgm.akademischer_titel"/>
                    <xs:alternative test="@role = 'Geschlecht'" type="wgm.geschlecht"/>
                    <xs:alternative
                        test="@role = 'erlernter Beruf' or @role = 'Beruf bei Antragstellung'"
                        type="wgm.beruf"/>
                    <xs:alternative test="@role = 'Beziehung zur verfolgten Person'"
                        type="wgm.beziehung"/>
                    <xs:alternative test="@role = 'Verfolgungsgrund'" type="wgm.verfolgungsgrund"/>
                    <xs:alternative test="@role = 'Verfolgungsgrund Spezifizierung'"
                        type="wgm.verfolgungsgrund_spezifizierung"/>
                    <xs:alternative test="@role = 'Verfolgungsgegenstand'"
                        type="wgm.verfolgungsgegenstand"/>
                    <xs:alternative
                        test="@role = 'Illegalität' or @role = 'Zwangsarbeit' or @role = 'Emigration'"
                        type="wgm.illegalitaet_zwangsarbeit_emigration"/>
                    <xs:alternative test="@role = 'Zwangssterilisierung'"
                        type="wgm.zwangssterilisierung"/>
                    <xs:alternative
                        test="@role = 'Deportation, Inhaftierung, Anstaltsunterbringung'"
                        type="wgm.deportation"/>
                </xs:element>
                <xs:element name="corpname" type="wgm.corpname"/>
                <xs:element name="persname" type="wgm.persname"/>
                <xs:sequence>
                    <xs:element name="geogname" minOccurs="1" maxOccurs="1">
                        <xs:alternative
                            test="@role = 'Geburtsort' or @role = 'Sterbeort vor 1947' or @role = 'letzter Wohnort' or @role = 'Wohnort bei Antragstellung' or @role = 'Sitz'"
                            type="wgm.ort"/>
                        <xs:alternative
                            test="@role = 'Staatsangehörigkeit vor der Verfolgung' or @role = 'Staatsangehörigkeit bei Antragstellung'"
                            type="wgm.staatsangehoerigkeit"/>
                    </xs:element>
                </xs:sequence>
                <xs:element name="note" minOccurs="0">
                    <xs:alternative
                        test="@label = 'Geburtsort' or @role = 'Sterbeort vor 1947' or @role = 'letzter Wohnort' or @role = 'Wohnort bei Antragstellung' or @role = 'Sitz'"
                        type="wgm.ort_note"/>
                    <xs:alternative test="not(@label)" type="wgm.note"/>
                </xs:element>
                <xs:element name="date" type="wgm.date"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
